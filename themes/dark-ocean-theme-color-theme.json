      "panel.background": "#070707",
      "panel.border": "#AF6DC5",
      "panelTitle.activeBorder": "#AF6DC5",

      "badge.background": "#AF6DC5",
      "badge.foreground": "#06080b",

      "editorWidget.background": "#0d0d0d",
      "editorWidget.foreground": "#d8d8d8",

      "breadcrumb.background": "#0d0d0d",
      "breadcrumbPicker.background": "#0d0d0d",
      "breadcrumb.activeSelectionForeground": "#AF6DC5",

      "dropdown.background": "#333333",

      "scrollbar.shadow": "#06080b",

      "gitDecoration.ignoredResourceForeground": "#3f4357"
